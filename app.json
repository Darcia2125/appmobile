{
  "expo": {
    "name": "emagnatitry",
    "slug": "emagnatitry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }, 
      "config": {
        "googleMaps": {
           "apiKey": "AIzaSyCfeV-dI5JYOl39mJDqsyL8xtRykQHU29I"
        }
     }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }, 
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow e-magnatitry to access your camera."
        }
      ], 
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-media-library",
        {
           "photosPermission": "Allow e-magnatitry to access your photos.",
           "savePhotosPermission": "Allow e-magnatitry to save photos.",
           "isAccessMediaLocationEnabled": true
        }
     ]

    ]
  }
}
